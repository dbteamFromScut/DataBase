    student_id, paper_id, question_id, commit_answer
  </sql>
  <insert id="insert" parameterType="com.db.model.StudentQuestionDone" >
    insert into sq_done (student_id, paper_id, question_id, 
      commit_answer)
    values (#{studentId,jdbcType=VARCHAR}, #{paperId,jdbcType=INTEGER}, #{questionId,jdbcType=INTEGER}, 
      #{commitAnswer,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.db.model.StudentQuestionDone" >
    insert into sq_done
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="studentId != null" >
        student_id,
      </if>
      <if test="paperId != null" >
        paper_id,
      </if>
      <if test="questionId != null" >
        question_id,
      </if>
      <if test="commitAnswer != null" >
        commit_answer,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="studentId != null" >
        #{studentId,jdbcType=VARCHAR},
      </if>
      <if test="paperId != null" >
        #{paperId,jdbcType=INTEGER},
      </if>
      <if test="questionId != null" >
        #{questionId,jdbcType=INTEGER},
      </if>
      <if test="commitAnswer != null" >
        #{commitAnswer,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <select id="selectByStudentId" parameterType="java.lang.String" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from sq_done
    where student_id=#{studentId}
  </select>
</mapper>